//https://www.geeksforgeeks.org/problems/nth-node-from-end-of-linked-list/1

/* Structure of node
class Node
{
    int data;
    Node next;
    Node(int d) {data = d; next = null; }
} */

class Solution {

    // Function to find the data of kth node from
    // the end of a linked list.
    int getKthFromLast(Node head, int k) {
        // Your code here
        int i=0;
        
        Node temp=head;
        while(temp!=null)
        {
            temp=temp.next;
            i++;
        }
       // System.out.print(i);
        if(k>i)
        return -1;
        
        k=i-k;
        while(head!=null)
        {
            if(k==0)
            {
                return head.data;
            }
            head=head.next;
            k--;
        }
        return -1;
    }
}
