//https://www.geeksforgeeks.org/problems/search-in-linked-list-1664434326/1


/*
  class Node {
   int data;
    Node next;

    Node(int d)  { data = d;  next = null; }
}
*/
class Solution {
    public boolean searchKey(Node head, int key) {
        // Code here
        //Node temp=head;
       if(head==null)
        return false;
        if(key==head.data)
        return true;
        return searchKey(head.next,key);
    }
}
