https://leetcode.com/problems/binary-string-with-substrings-representing-1-to-n/

class Solution {
    public boolean queryString(String s, int n) {
        Queue<String> q=new LinkedList<>();
        q.add("1");
        int i=1;
        while(i<=n)
        {
            String x=q.remove();
            if(!s.contains(x))
            return false;
            q.add(x+"0");
            q.add(x+"1");
            i++;
        }
        return true;
    }
}
